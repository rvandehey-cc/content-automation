{
  "name": "headless-scraper",
  "version": "2.0.0",
  "description": "A robust headless browser scraper for Cloudflare-protected websites with modular architecture",
  "main": "src/cli/automation.js",
  "type": "module",
  "scripts": {
    "start": "node src/cli/automation.js",
    "dev": "NODE_ENV=development node src/cli/automation.js",
    "scrape": "node src/cli/scraper.js",
    "process": "node src/cli/processor.js",
    "xml": "node src/cli/xml.js",
    "install-browsers": "playwright install",
    "test": "echo \"Tests not implemented yet\" && exit 0",
    "lint": "eslint src/**/*.js",
