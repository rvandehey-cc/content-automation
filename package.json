{
  "name": "headless-scraper",
  "version": "2.0.0",
  "description": "A robust headless browser scraper for Cloudflare-protected websites with modular architecture",
  "main": "src/cli/automation.js",
  "type": "module",
  "scripts": {
    "start": "node src/cli/automation.js",
    "dev": "NODE_ENV=development node src/cli/automation.js",
    "scrape": "node src/cli/scraper.js",
    "process": "node src/cli/processor.js",
    "xml": "node src/cli/xml.js",
    "install-browsers": "playwright install",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "lint": "eslint src/**/*.js",
    "clean": "node src/cli/cleanup.js",
    "docs": "echo \"Documentation generation not implemented yet\" && exit 0",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "dev:web": "next dev",
    "build": "next build",
    "start:web": "next start",
    "lint:web": "next lint",
    "prepare": "husky"
  },
  "dependencies": {
    "@prisma/client": "^6.19.1",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0",
    "autoprefixer": "^10.4.23",
    "blessed-contrib": "^4.11.0",
    "cheerio": "^1.0.0-rc.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.6.1",
    "fs-extra": "^11.1.1",
    "jsdom": "^23.2.0",
    "lucide-react": "^0.562.0",
    "next": "^16.1.1",
    "node-fetch": "^3.3.2",
    "p-queue": "^7.4.1",
    "playwright": "^1.40.0",
    "postcss": "^8.5.6",
    "prisma": "^6.19.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "reblessed": "^0.2.1",
    "tailwind-merge": "^3.4.0",
    "user-agents": "^1.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^8.55.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "tailwindcss": "^3.4.19",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "scraper",
    "headless",
    "browser",
    "cloudflare",
    "playwright",
    "wordpress",
    "content-automation",
    "modular"
  ],
  "author": "Content Automation Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  }
}
