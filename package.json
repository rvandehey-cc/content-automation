    "docs": "echo \"Documentation generation not implemented yet\" && exit 0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.6.1",
    "fs-extra": "^11.1.1",
    "jsdom": "^23.2.0",
    "node-fetch": "^3.3.2",
    "p-queue": "^7.4.1",
    "playwright": "^1.40.0",
    "user-agents": "^1.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0"
  },
  "keywords": [
    "scraper",
    "headless",
    "browser",
    "cloudflare",
    "playwright",
    "wordpress",
    "content-automation",
    "modular"
